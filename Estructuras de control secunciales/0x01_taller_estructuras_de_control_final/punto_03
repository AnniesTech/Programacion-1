/*
 *  Imprimir la tabla de multiplicar ded un numero ingresado por el usuario.
 */
package tablademultiplicar;
import java.util.Scanner;
/**
 *
 * @author Amarie
 */
public class TablaDeMultiplicar {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Scanner read = new Scanner(System.in);
        int num = 0;
        int multi = 0;
        System.out.print("Ingrese el numero del que desea saber su tabla de multiplicar: ");
        num = read.nextInt();
        
        for(int i = 1; i<=10; i++){
            multi = num * i;
            System.out.println(num + " x "+i+" = "+multi);
        }
    }
    
}
